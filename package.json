{
  "name": "symmetric-js",
  "version": "1.0.9",
  "description": "Symmetric provides observable model and collection classes with support for field specifications that mirror backend sources with encoding functions and validation rules.",
  "keywords": [
    "react",
    "observable",
    "model",
    "collection",
    "rest",
    "api",
    "fetch",
    "backbone",
    "symmetric"
  ],
  "main": "dist/symmetric.js",
  "files": [
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "release": "yarn build && git tag $npm_package_version && git push --tags && yarn publish --no-lockfile",
    "test": "node test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:symmetricapi/symmetric-js.git"
  },
  "homepage": "https://github.com/symmetricapi/symmetric-js",
  "bugs": {
    "url": "https://github.com/symmetricapi/symmetric-js/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^6.0.1",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "node-fetch": "^2.1.2",
    "prettier": "^1.18.2",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 100
  }
}
